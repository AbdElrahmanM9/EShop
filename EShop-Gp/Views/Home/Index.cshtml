<div id="myCarouselz" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators bg-dark">
        <li data-target="#myCarouselz" data-slide-to="0" class="active"></li>
        <li data-target="#myCarouselz" data-slide-to="1"></li>
        <li data-target="#myCarouselz" data-slide-to="2"></li>
        <li data-target="#myCarouselz" data-slide-to="3"></li>
        <li data-target="#myCarouselz" data-slide-to="4"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/Image/s1.jpg">
            <div class="container">
                <button type="button" class="btn btn-outline-warning btn-lg">
                    CONTACT US
                </button>
                <button type="button" class="btn btn-warning btn-lg">
                    Get Started
                </button>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/Image/4.jpg">
        </div>
        <div class="carousel-item">
            <img src="~/Image/cover.jpg">
        </div>
        <div class="carousel-item">
            <img src="~/Image/background.jpg">
        </div>
        <div class="carousel-item">
            <img src="~/Image/background1.jpg">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-target="#myCarouselz" data-slide="prev">
        <span class="carousel-control-prev-icon bg-dark" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-target="#myCarouselz" data-slide="next">
        <span class="carousel-control-next-icon bg-dark" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </button>
</div>


